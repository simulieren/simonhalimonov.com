{
  "name": "simonhalimonov.com",
  "private": true,
  "description": "A Gatsby starter template for a Wordpress blog, portfolio, built using Typescript, Theme UI",
  "version": "0.1.0",
  "author": "Simon Halimonov <hello@simonhalimonov.de>",
  "homepage": "https://github.com/simulieren/simonhalimonov.com",
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "gatsby-starter-template",
    "gatsby-typescript",
    "gatsby-social-widgets",
    "gatsby-twitter",
    "social-widgets"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run purgeCache && cp ./src/environments/.env.production ./.env.production && gatsby build",
    "start": "npm run purgeCache && cp ./src/environments/.env.development ./.env.development && gatsby develop",
    "serve": "npm run purgeCache && cp ./src/environments/.env.development ./.env.development && gatsby build && gatsby serve",
    "purgeAll": "rm -rf node_modules && rm -rf yarn.lock rm -rf package-lock.json && rm -rf .cache && rm -rf public && npm install",
    "purgeCache": "rm -rf .cache && rm -rf public",
    "purgeDependencies": "rm -rf node_modules && rm -rf yarn.lock rm -rf package-lock.json && npm install"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@mdx-js/react": "^1.6.0",
    "@types/theme-ui": "^0.3.1",
    "framer-motion": "^1.10.3",
    "gatsby": "^2.0.0",
    "gatsby-image": "^2.0.0",
    "gatsby-paginate": "^1.0.0",
    "gatsby-plugin-advanced-sitemap": "^1.0.0",
    "gatsby-plugin-catch-links": "^2.1.13",
    "gatsby-plugin-google-analytics": "^2.0.0",
    "gatsby-plugin-import": "^2.0.0",
    "gatsby-plugin-manifest": "^2.0.0",
    "gatsby-plugin-offline": "^3.0.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.0.0",
    "gatsby-plugin-sass": "^2.0.0",
    "gatsby-plugin-sharp": "^2.0.0",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-source-filesystem": "^2.0.0",
    "gatsby-source-instagram": "^0.5.0",
    "gatsby-source-wordpress": "^3.0.0",
    "gatsby-transformer-sharp": "^2.0.0",
    "gatsby-wordpress-inline-images": "^1.0.0",
    "html-react-parser": "^0.10.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-feather": "^2.0.8",
    "react-helmet": "^5.0.0",
    "react-scroll-parallax": "^2.3.2",
    "react-syntax-highlighter": "^12.2.1",
    "react-use": "^14.2.0",
    "theme-ui": "^0.3.1",
    "typeface-ibm-plex-sans": "^0.0.75",
    "typeface-ibm-plex-serif": "^0.0.75"
  },
  "devDependencies": {
    "@types/enzyme": "^3.0.0",
    "@types/enzyme-adapter-react-16": "^1.0.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/react-helmet": "^5.0.0",
    "@types/react-test-renderer": "^16.0.0",
    "babel-plugin-import": "^1.0.0",
    "dotenv": "^8.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "node-sass": "^4.0.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.0.0",
    "source-map-explorer": "^2.0.0",
    "ts-node": "^8.10.0",
    "typescript": "^3.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/simulieren/simonhalimonov.com"
  },
  "bugs": {
    "url": "https://github.com/simulieren/simonhalimonov.com/issues"
  }
}
